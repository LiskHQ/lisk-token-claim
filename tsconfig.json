{
	"compilerOptions": {
		"forceConsistentCasingInFileNames": true,
		"target": "es2022",
		"module": "commonjs",
		"declaration": true,
		"moduleResolution": "node",
		"newLine": "lf",
		"noFallthroughCasesInSwitch": true,
		"noImplicitReturns": false,
		"noImplicitAny": false,
		"noUnusedLocals": false,
		"noUnusedParameters": false,
		"pretty": true,
		"removeComments": true,
		"resolveJsonModule": true,
		"sourceMap": true,
		"strict": true,
		"composite": true,
		"esModuleInterop": true
	}
}
